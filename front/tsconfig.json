{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@hooks/*": ["src/hooks/*"],
      "@_actions/*": ["src/_actions/*"],
      "@_reducers/*": ["src/_reducers/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@hocs/*":["src/hocs/*"],
      "@store/*":["src/store/*"],
      "@typings/*": ["src/typings/*"],
      "@image/*":["src/image/*"],
    }
  },
  "include": [
    "src"
  ]
}
